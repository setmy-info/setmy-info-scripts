TENSORFLOW_NAME=tensorflow
TENSORFLOW_VERSION=2.15.1
SYSTEM_PROFILES_DIR=$(smi-lib-location)/profiles

tensorflow_download_func() {
    CUR_DIR=$(pwd)
    cd ${CUR_DIR}
}

tensorflow_install_func() {
    CUR_DIR=$(pwd)
    smi-venv-command pip install jupyterlab notebook voila
    smi-venv-command pip install matplotlib numpy pandas pyarrow
    smi-venv-command pip install seaborn click colorama
    cd ${CUR_DIR}
}
