SCIKIT_LEARN_NAME=scikit
SCIKIT_LEARN_VERSION=2.15.1
SYSTEM_PROFILES_DIR=$(smi-lib-location)/profiles

scikit_download_func() {
    CUR_DIR=$(pwd)
    cd ${CUR_DIR}
}

scikit_install_func() {
    CUR_DIR=$(pwd)
    smi-venv-command pip install scikit-learn
    smi-venv-command pip install matplotlib numpy pandas pyarrow
    cd ${CUR_DIR}
}
